<h1><%= @artist.name %></h1>
<%= image_tag @artist.image_path %>

<ul>
  <%= @artist.songs.each do |song| %>
  <li><%= song.title %></li>
  <% end %>
</ul>

<%= link_to "New song", new_artist_song_path(@artist) %>

<%= link_to "Edit Artist", edit_artist_path %>

<%= link_to "Delete", artist_path(@artist), method: :delete %>
